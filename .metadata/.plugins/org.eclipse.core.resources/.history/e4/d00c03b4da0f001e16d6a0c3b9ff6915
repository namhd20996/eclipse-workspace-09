
import static org.junit.Assert.assertEquals;

import org.junit.After;
import org.junit.AfterClass;
import org.junit.Before;
import org.junit.BeforeClass;
import org.junit.Test;

import model.Calculator;

public class MathLibTest {
	
	private Calculator calculator;
	

	@BeforeClass
	public static void setUpBeforeClass() throws Exception {
		System.out.println("setUpBeforeClass class");
	}

	@AfterClass
	public static void tearDownAfterClass() throws Exception {
		System.out.println("tearDownAfterClass class");
	}

	@Before
	public void setUp() throws Exception {
		System.out.println("setUp class");
		calculator = new Calculator();
	}

	@After
	public void tearDown() throws Exception {
		System.out.println("tearDown class");
		calculator = null;
	}

	@Test
	public void testPhepTru() {
		System.out.println("testPhepTru class");
		assertEquals(10, calculator.phepTru(15.0, 5.0), 0.01);
	}

	@Test
	public void testPhepCong() {
		System.out.println("testPhepCong class");
		assertEquals(10, calculator.phepCong(15.0, 5.0), 0.01);
	}

}
